// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model MutualAid {
  id            Int         @default(autoincrement()) @id
  name          String      @db.VarChar(255)
  neighborhood  String
  phone         String
  email         String
  website       String
  need_help     String
  give_help     String
}