generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model MutualAid {
  id           Int    @id @default(autoincrement())
  name         String @db.VarChar(255)
  neighborhood String
  phone        String
  email        String
  website      String
  need_help    String
  give_help    String
}
